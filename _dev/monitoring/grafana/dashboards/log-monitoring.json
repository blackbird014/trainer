        "title": "Recent Logs (All Services)",
        "targets": [
          {
            "expr": "{job=~\".+\"}",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "loki"
            }
          }
        ],
        "options": {
          "dedupStrategy": "none",
          "enableLogDetails": true,
          "prettifyLogMessage": false,
          "showCommonLabels": false,
          "showLabels": true,
          "showTime": true,
