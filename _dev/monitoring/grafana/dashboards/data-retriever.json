{
  "title": "Data Retriever Dashboard",
  "tags": ["data-retriever", "retrieval", "trainer"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "title": "Retrieval Operations per Second",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(data_retriever_operations_total[5m])",
          "legendFormat": "{{source}} - {{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "id": 2,
      "title": "Retrieval Duration (P95)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "histogram_quantile(0.95, rate(data_retriever_operation_duration_seconds_bucket[5m]))",
          "legendFormat": "{{source}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 3,
      "title": "Total Operations",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(data_retriever_operations_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 4,
      "title": "Success Rate",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(rate(data_retriever_operations_total{status=\"success\"}[5m])) / sum(rate(data_retriever_operations_total[5m])) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "id": 5,
      "title": "Total Errors",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(data_retriever_errors_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 10, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Active Operations",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(data_retriever_active_operations)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 7,
      "title": "Cache Hit Ratio",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(data_retriever_cache_hits_total[5m]) / (rate(data_retriever_cache_hits_total[5m]) + rate(data_retriever_cache_misses_total[5m]))",
          "legendFormat": "{{source}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "id": 8,
      "title": "Cache Hits vs Misses",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(data_retriever_cache_hits_total[5m])",
          "legendFormat": "Hits - {{source}}",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(data_retriever_cache_misses_total[5m])",
          "legendFormat": "Misses - {{source}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "id": 9,
      "title": "Data Size Retrieved",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(data_retriever_data_size_bytes_sum[5m])",
          "legendFormat": "{{source}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "id": 10,
      "title": "Errors by Type",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(data_retriever_errors_total[5m])",
          "legendFormat": "{{source}} - {{error_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 0.1, "color": "yellow"},
              {"value": 1, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 11,
      "title": "Operations by Source",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum by (source) (data_retriever_operations_total)",
          "legendFormat": "{{source}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 12,
      "title": "Average Data Size per Request",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(data_retriever_data_size_bytes_sum[5m]) / rate(data_retriever_data_size_bytes_count[5m])",
          "legendFormat": "{{source}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    }
  ]
}

