{
  "id": null,
  "uid": "data-store",
  "title": "Data Store Dashboard",
  "tags": ["data-store", "storage", "persistence"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Operations Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "rate(data_store_operations_total[5m])",
          "legendFormat": "{{operation}} - {{status}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "id": 2,
      "title": "Operation Duration",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(data_store_operation_duration_seconds_bucket[5m]))",
          "legendFormat": "p95 - {{operation}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.50, rate(data_store_operation_duration_seconds_bucket[5m]))",
          "legendFormat": "p50 - {{operation}}",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 3,
      "title": "Total Operations",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "sum(increase(data_store_operations_total[1h]))",
          "legendFormat": "Total",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short"
        }
      }
    },
    {
      "id": 4,
      "title": "Error Rate",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
      "targets": [
        {
          "expr": "sum(rate(data_store_errors_total[5m]))",
          "legendFormat": "Errors/sec",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 0.1},
              {"color": "red", "value": 1}
            ]
          },
          "unit": "ops"
        }
      }
    },
    {
      "id": 5,
      "title": "Active Operations",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {
          "expr": "data_store_active_operations",
          "legendFormat": "{{operation}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 6,
      "title": "Data Size Distribution",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(data_store_data_size_bytes_bucket[5m]))",
          "legendFormat": "p95 - {{operation}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.50, rate(data_store_data_size_bytes_bucket[5m]))",
          "legendFormat": "p50 - {{operation}}",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "id": 7,
      "title": "Operations by Type",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
      "targets": [
        {
          "expr": "sum(rate(data_store_operations_total[5m])) by (operation)",
          "legendFormat": "{{operation}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ]
    },
    {
      "id": 8,
      "title": "Error Breakdown",
      "type": "table",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
      "targets": [
        {
          "expr": "sum(rate(data_store_errors_total[5m])) by (operation, error_type)",
          "format": "table",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {"Time": true},
            "indexByName": {},
            "renameByName": {
              "operation": "Operation",
              "error_type": "Error Type",
              "Value": "Rate"
            }
          }
        }
      ]
    },
    {
      "id": 9,
      "title": "Total Items in Store",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 28},
      "targets": [
        {
          "expr": "data_store_items_total",
          "legendFormat": "{{backend}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short"
        }
      }
    },
    {
      "id": 10,
      "title": "Success Rate",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 28},
      "targets": [
        {
          "expr": "sum(rate(data_store_operations_total{status=\"success\"}[5m])) / sum(rate(data_store_operations_total[5m])) * 100",
          "legendFormat": "Success %",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 90},
              {"color": "green", "value": 99}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    }
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "weekStart": ""
}
