    {
      "id": 11,
      "title": "LLM Provider - Total Cost",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(llm_provider_cost_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 4
        }
      }
    },
    {
      "id": 12,
      "title": "LLM Provider - Error Rate",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(rate(llm_provider_errors_total[5m])) / sum(rate(llm_provider_requests_total[5m])) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 5, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 13,
      "title": "Total Cost Across All Modules",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 16},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(prompt_manager_cost_total) + sum(llm_provider_cost_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 4
        }
      }
    },
    {
      "id": 14,
      "title": "Module Health Status",
      "type": "table",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "up{job=~\"prompt-manager|prompt-security|llm-provider|data-retriever|format-converter|model-trainer|test-agent\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "displayMode": "color-background"
          },
          "mappings": [
            {
              "options": {
                "1": {
                  "text": "UP",
                  "color": "green"
                },
                "0": {
                  "text": "DOWN",
                  "color": "red"
                }
              },
              "type": "value"
            }
          ]
        }
      }
    },
    {
