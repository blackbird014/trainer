{
  "title": "Model Trainer Dashboard",
  "tags": ["model-trainer", "training", "ml", "trainer"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "title": "Training Runs per Second",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(model_trainer_training_runs_total[5m])",
          "legendFormat": "{{framework}} - {{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "id": 2,
      "title": "Training Duration (P95)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "histogram_quantile(0.95, rate(model_trainer_training_duration_seconds_bucket[5m]))",
          "legendFormat": "{{framework}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 3,
      "title": "Total Training Runs",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(model_trainer_training_runs_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 4,
      "title": "Success Rate",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(rate(model_trainer_training_runs_total{status=\"success\"}[5m])) / sum(rate(model_trainer_training_runs_total[5m])) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "id": 5,
      "title": "Active Training Runs",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(model_trainer_active_training_runs)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 6,
      "title": "Total Model Versions",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(model_trainer_model_versions_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 7,
      "title": "Training Runs by Framework",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum by (framework) (model_trainer_training_runs_total)",
          "legendFormat": "{{framework}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 8,
      "title": "Dataset Sizes",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(model_trainer_dataset_size_sum[5m])",
          "legendFormat": "{{dataset_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 9,
      "title": "Evaluation Runs",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(model_trainer_evaluation_runs_total[5m])",
          "legendFormat": "{{model_version}} - {{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "id": 10,
      "title": "Checkpoint Sizes",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(model_trainer_checkpoint_size_bytes_sum[5m])",
          "legendFormat": "{{framework}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "id": 11,
      "title": "Errors by Type",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(model_trainer_errors_total[5m])",
          "legendFormat": "{{error_type}} - {{framework}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 0.1, "color": "yellow"},
              {"value": 1, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 12,
      "title": "Average Training Duration",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(model_trainer_training_duration_seconds_sum[5m]) / rate(model_trainer_training_duration_seconds_count[5m])",
          "legendFormat": "{{framework}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    }
  ]
}

