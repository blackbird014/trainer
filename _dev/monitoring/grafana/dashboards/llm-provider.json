{
  "title": "LLM Provider Dashboard",
  "tags": ["llm-provider", "llm", "trainer"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "title": "LLM Requests per Second",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(llm_provider_requests_total[5m])",
          "legendFormat": "{{provider}} - {{model}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "id": 2,
      "title": "Request Duration (P95)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "histogram_quantile(0.95, rate(llm_provider_request_duration_seconds_bucket[5m]))",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 3,
      "title": "Total Requests",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(llm_provider_requests_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 4,
      "title": "Total Tokens",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(llm_provider_tokens_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 5,
      "title": "Total Cost (USD)",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(llm_provider_cost_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 4
        }
      }
    },
    {
      "id": 6,
      "title": "Error Rate",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum(rate(llm_provider_errors_total[5m])) / sum(rate(llm_provider_requests_total[5m])) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 5, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 7,
      "title": "Tokens per Request",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(llm_provider_tokens_total[5m]) / rate(llm_provider_requests_total[5m])",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 8,
      "title": "Cost per Request",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(llm_provider_cost_total[5m]) / rate(llm_provider_requests_total[5m])",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 6
        }
      }
    },
    {
      "id": 9,
      "title": "Requests by Provider",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "sum by (provider) (llm_provider_requests_total)",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 10,
      "title": "Errors Over Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "__datasource_uid__"
          },
          "expr": "rate(llm_provider_errors_total[5m])",
          "legendFormat": "{{provider}} - {{error_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 0.1, "color": "yellow"},
              {"value": 1, "color": "red"}
            ]
          }
        }
      }
    }
  ]
}

